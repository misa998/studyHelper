<BorderPane fx:id="borderPane" prefHeight="600.0" prefWidth="840.0" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.studyhelper.controller.MainController">
   <center>
      <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab onSelectionChanged="#dashboardTabChanged" text="Dashboard">
            <content>
              <AnchorPane fx:id="dashboardTabAnchorPane" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>

                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="coursesTab" onSelectionChanged="#courseTabChange" text="Courses">
            <content>
              <AnchorPane fx:id="coursesTabAnchorPane" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>

                  </children>
             </AnchorPane>
            </content>
          </Tab>
            <Tab onSelectionChanged="#pomodoroTabChange" text="Pomodoro">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" fx:id="pomodoroTabAnchorPane">
