                                             <font>
                                                <Font name="System Bold" size="18.0" />
                                             </font></Label>
                                          <Label fx:id="labelBody" text="Body" StackPane.alignment="BOTTOM_LEFT">
                                             <StackPane.margin>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                             </StackPane.margin></Label>
                                       </children>
                                    </StackPane>
                                 </children>
                              </VBox>
                              <Button layoutX="587.0" layoutY="14.0" mnemonicParsing="false" text="Add" />
                           </children>
