                </content>
            </Tab>
            <Tab disable="true" text="Motivation">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Pane fx:id="coursePanel1" layoutX="8.0" layoutY="7.0" prefHeight="343.0" prefWidth="639.0">
                           <children>
                              <VBox fx:id="vBox1" layoutX="14.0" layoutY="14.0" prefHeight="205.0" prefWidth="543.0" spacing="15.0">
                                 <children>
                                    <StackPane fx:id="stac" prefHeight="81.0" prefWidth="488.0">
                                       <children>
                                          <Rectangle fx:id="rectangle" arcHeight="25.0" arcWidth="25.0" fill="#8f8f8fb4" height="81.0" strokeType="OUTSIDE" styleClass="rectangle" width="543.0">
                                             <stroke>
                                                <LinearGradient endX="1.0" endY="1.0">
                                                   <stops>
                                                      <Stop>
                                                         <color>
                                                            <Color opacity="0.6695652008056641" />
                                                         </color>
                                                      </Stop>
                                                      <Stop offset="1.0">
                                                         <color>
                                                            <Color red="1.0" green="1.0" blue="1.0" />
